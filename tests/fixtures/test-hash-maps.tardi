
uses: std/hashmaps
uses: std/io
uses: std/strings
uses: std/vectors

"<hashmap> : " print
<hashmap> .

">hashmap : " print
{ { 'a' 1 } { 'b' 2 } { 'c' 3 } } >hashmap
is-hashmap? .

"H{ : " print
H{ { 'a' 4 } { 'b' 5 } { 'c' 6 } }
{ { 'a' 4 } { 'b' 5 } { 'c' 6 } } >hashmap
== .

"== : " print
H{ { 'a' 4 } { 'b' 5 } { 'c' 6 } }
H{ { 'a' 4 } { 'b' 5 }  } == .

">string : " print
H{ { "given-name" "Trillian" } { "surname" "McMillan" } } strings/>string .

"length : " print
H{ } hashmaps/length .
H{ { 1 2 } } hashmaps/length .
H{ { 1 2 } { 2 3 } { 3 4 } { 4 5 } }
dup hashmaps/length .

">vector : " print
dup >vector dup sort! .

"keys : " print
dup keys dup sort! .

"values : " print
dup values dup sort! .


"in? : " print
1 over hashmaps/in? .
9 over hashmaps/in? .

"get : " print
2 over get . .
9 over get . .

"add! : " print
H{ { 'a' 1 } { 'b' 2 } { 'd' 4 } { 'e' 5 } }
{ 'c' 3 } over add!
>vector dup sort! .

