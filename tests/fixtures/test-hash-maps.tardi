
uses: std/hashmaps
uses: std/io
uses: std/strings

"<hashmap> : " print
<hashmap> .

">hashmap : " print
{ { 'a' 1 } { 'b' 2 } { 'c' 3 } } >hashmap
is-hashmap? .

"H{ : " print
H{ { 'a' 4 } { 'b' 5 } { 'c' 6 } }
{ { 'a' 4 } { 'b' 5 } { 'c' 6 } } >hashmap
== .

"== : " print
H{ { 'a' 4 } { 'b' 5 } { 'c' 6 } }
H{ { 'a' 4 } { 'b' 5 }  } == .

">string : " print
H{ { "given-name" "Trillian" } { "surname" "McMillan" } } strings/>string .

"length : " print
H{ } hashmaps/length .
H{ { 1 2 } } hashmaps/length .
H{ { 1 2 } { 2 3 } { 3 4 } { 4 5 } } hashmaps/length .

