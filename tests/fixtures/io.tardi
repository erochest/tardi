"io/close"
[ reach ] dip swap close
pick rm

"tmp/temporary-three.txt" <writer>
dup close

"fs/truncate"
"tmp/test-truncate.txt" 
"this has some content\n" over write-file drop
dup truncate swap
dup read-file drop
swap rm

