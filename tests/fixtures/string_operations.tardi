use: std/strings

// Test creating an empty string
<string> >string
// Expected: "\"\""

// Test converting values to strings
42 >string
// Expected: "42"

3.14 >string
// Expected: "3.14"

#t >string
// Expected: "#t"

'A' >string
// Expected: "A"

// Test UTF-8 to string conversion
{ 72 101 108 108 111 } utf8>string
// Expected: "Hello"

// Test string concatenation
"Hello, " "world!" concat
// Expected: "Hello, world!"

// Test empty string concatenation
"" "test" concat
// Expected: "test"

"prefix" "" concat
// Expected: "prefix"

// Test multiple concatenations
"a" "b" concat "c" concat "d" concat
// Expected: "abcd"
